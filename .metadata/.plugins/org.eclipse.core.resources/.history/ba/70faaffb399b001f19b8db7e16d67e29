package Model;

import java.io.File;

import View.Frame;

public class ChessBoard {
	int x;
	int y;
	int width;
	int height;
	File image;
	public ChessBoard(int x, int y, int width, int height) {
		super();
		this.x = x;
		this.y = y;
		this.width = width;
		this.height = height;
		image=  new File("D:\\Java_Project\\AI\\GameCoTuong\\src\\Board_Image\\Xiangqi_board.png");
	}
	
}
